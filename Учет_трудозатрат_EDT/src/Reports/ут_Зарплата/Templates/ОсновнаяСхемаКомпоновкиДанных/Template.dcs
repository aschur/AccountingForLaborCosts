ВЫБРАТЬ
	ут_ТекущийУровень.Период КАК ПериодС,
	ЕСТЬNULL(МИНИМУМ(ут_ТекущийУровень1.Период), ДАТАВРЕМЯ(2050, 1, 1)) КАК ПериодДо,
